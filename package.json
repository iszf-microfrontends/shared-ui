{
  "name": "@iszf-microfrontends/shared-ui",
  "version": "0.0.2",
  "description": "Shared UI",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "author": "Sviatoslav Nazin <sviatoslavnazin@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/iszf-microfrontends/shared-ui.git"
  },
  "license": "ISC",
  "scripts": {
    "prepare": "husky install",
    "clean": "rimraf dist",
    "dev": "ladle serve",
    "build": "pnpm clean && rollup -c",
    "test": "jest",
    "lint": "eslint . --ext ts,tsx",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier . --write --ignore-path node_modules/@iszf-microfrontends/prettier-config/.prettierignore",
    "prep": "pnpm format && pnpm lint:fix && git add .",
    "release": "pnpm test && pnpm build && npm publish"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{js,cjs}": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.1",
    "@mantine/core": "^6.0.16",
    "@mantine/hooks": "^6.0.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@ladle/react": "^3.0.0",
    "@mantine/core": "^6.0.16",
    "@mantine/hooks": "^6.0.16",
    "@tabler/icons-react": "^2.38.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@iszf-microfrontends/eslint-config": "^0.0.5",
    "@iszf-microfrontends/prettier-config": "^0.0.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.4",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "eslint": "^8.51.0",
    "eslint-plugin-jest": "^27.4.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.29.2",
    "rollup-plugin-dts": "^6.0.2",
    "rollup-plugin-esbuild": "^6.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
